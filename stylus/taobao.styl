*
	padding 0
	margin 0
#top
	width 100%
	height 56px
	background-color rgb(255,255,255)
	border-bottom 4px solid rgb(0,204,204)
	#head
		margin 0 auto
		width 1190px
		height 50px
		background-color white
		position relative
		div
			float left
			line-height 50px
			font-size 14px
			font-family Microsoft Yahei	
			a
				text-decoration none
				padding 0 30px
				color black
				border-right 1px solid rgb(230,230,230)
			&:nth-child(3)
				a
					border transparent
			&:nth-child(4)
				float right
				a
					border transparent
			&:nth-child(5)
				float right
			&:hover
				a
					color rgb(0,204,204)
			.hidden1
				width 122px
				height 411px
				border 1px solid rgb(220,220,220)
				display none
				position absolute
				left 90px
				top 34px
				z-index 99999
				a	
					text-decoration none
					display block
					color black	
					border-top 1px dotted gray
					font-size 12px
					line-height 40px
					text-align center
					background-color rgb(255,255,255)
					&:first-child
						border transparent
				&:hover
					display block
				a:hover
					background-color rgb(0,204,204)	
			.hidden2
				width 140px
				height 112px
				border 1px solid rgb(220,220,220)
				display none
				position absolute
				top 34px
				a	
					text-decoration none
					display block
					color black	
					border-top 1px dotted gray
					font-size 12px
					line-height 36px
					text-align center
					background-color rgb(255,255,255)
					&:first-child
						border transparent
				&:hover
					display block
				a:hover
					background-color rgb(0,204,204)	
		div a:hover+.hidden1
			display block
		div a:hover+.hidden2
			display block  			
