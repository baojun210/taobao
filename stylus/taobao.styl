.fullNav
	width 1190px
	margin 0 auto
	.nav-title
		height 30px
		margin-top 63px
		background-image url(../img/hengxian.png)
		background-repeat repeat-x
		.title
			width 136px
			height 30px
			line-height 30px
			font-size 28px
			display inline-block
			text-align center
			position relative
			left 527px
			top -14px
			background-color rgb(255,255,255)
	.text
		text-align center
		font-size 14px
		color #8f8f8f
		position relative
		top -10px
	.first
		width 1190px
		.floor-info
			margin-bottom 10px
			color #5a5a5a
			width 80px
			a
				.num
					font-size 14px	
				.name
					font-size 20px
					margin-left 3px
					
				.icon
					font-size 20px
				.line
					display inline-block
					width 1093px
					border-bottom 1px dotted #e9e9e9
					position relative
					top -7px
					left 12px
			&:hover
				color rgb(0,204,206)
		.nav-list
			width 1210px
			zoom 1
			&:after
				visibility hidden
				clear both
				display block
				height 0px
				content "."
			li
				float left
				width 280px
				height 370px
				margin 0 18px 18px 0
				position relative
				.main-img
					position absolute
					right 0
					top 0
				.first-link
					display block
					position relative
					height 370px
					z-index 2
					.info
						display block
						width 260px
						height 63px
						color white
						position absolute
						z-index 2
						left 28px
						top 28px
						.name
							font-size 24px
							line-height 24px
						.money
							font-size 14px
							line-height 14px
							margin-top 10px
						.link
							display block
							width 100px
							height 32px
							background-color white
							text-align center
							font-size 14px
							line-height 32px
							margin-top 20px
							cursor pointer
							.text
								position relative
								left 4px
								top 0
								color #78c9e3
			.nav-item
				border 1px solid rgb(237,237,237)
				a
					position relative
					display block
					.project-area
						display block
						width 278px
						height 187px
						overflow hidden
						img
							transition all .3s linear
					.project-text
						display block
						width 250px
						margin 11px 15px
						color #2a2a2a
						height 42px
						line-height 22px
						font-size 15px
						transition all .3s linear
				&:hover
					.project-area
						img
							transform-origin 50% 50%
							transform scale(1.1,1.1)
					.project-text
						color #00ccce
				.tag-con
					display block
					width 250px
					margin 7px auto 22px
					height 23px
					overflow hidden
					.tag-link
						font-size 10px
						display inline-block
						height 23px
						color #999
						line-height 23px
						background-color #f0f0f0
						padding 0 4px
						margin-right 8px
						cursor pointer
						&:hover
							background-color rgb(210,210,210)
				.bar
					height 6px
					width 250px
					display block
					background-color rgb(237,237,237)
					border-radius 5px
					margin 0 auto
					
				.project
					display block
					margin-top 15px
					.other-info
						display inline-block	
						.info-deal
							width 80px
						.info-dollar
							width 110px
						.info-support
							width 66px		
						.every-info
							display inline-block
							em
								display inline-block
								width 100%
								text-align center
							.info-num
								font-size 14px
								line-height 14px
								color #333
							
							.info-name
								font-size 12px
								line-height 12px
								margin-top 5px
								color #999

						
					
						
						
		
				
			
		
